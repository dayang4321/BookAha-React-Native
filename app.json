{
  "expo": {
    "name": "BookAha",
    "slug": "bookaha",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms":["android"],
    "icon": "./app/assets/images/bird-logo-icon.png",
    "splash": {
      "image": "./app/assets/images/bird-logo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#080808"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android":{
      "package": "com.bookaha.bookaha",
      "versionCode": 1,
      "adaptiveIcon":{
        "foregroundImage": "./app/assets/images/bird-logo-adaptive.png",
        "backgroundColor": "#080808"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/images/bird-logo-icon.png"
    }
  }
}
